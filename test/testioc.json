/*
 * This file defines two groups equivalent to the groups defined in testiocg.db
 * Dependencies: testioc.db
 */

/*
 * Group george:tableExample2
 * Use +id mapping: to present as a epics NTTable
 * with two columns of doubles taken from $(user):vectorExampleD1 and $(user):vectorExampleD2
 * Get metadata (alarms, etc) from $(user):vectorExampleD1
 */
{
  "george:tableExample2": {
    "+id": "epics:nt/NTTable:1.0",
    "": {
      "+type": "meta",
      "+channel": "george:vectorExampleD1.VAL"
    },
    "labels": {
      "+channel": "george:groupExampleAS.VAL",
      "+type": "plain"
    },
    "value.A": {
      "+channel": "george:vectorExampleD1.VAL",
      "+type": "plain",
      "+putorder": 1
    },
    "value.B": {
      "+channel": "george:vectorExampleD2.VAL",
      "+type": "plain",
      "+putorder": 1
    },
    "_save": {
      "+channel": "george:groupExampleSave.VAL",
      "+type": "proc",
      "+putorder": 2,
      "+trigger": "*"
    }
  },
  /*
   * Group george:structExample2
   * This is an example of a structure that does not correspond to an EPICS normative type
   */
  "george:structExample2": {
    "ai": {
      "+channel": "george:aiExample.VAL",
      "+putorder": 0
    },
    "calc": {
      "+channel": "george:calcExample.VAL",
      "+putorder": 1
    },
    "string": {
      "+channel": "george:stringExample.VAL",
      "+putorder": 0
    },
    "array": {
      "+channel": "george:arrayExample.VAL",
      "+putorder": 0
    },
    "sa[0].long": {
      "+channel": "george:longExample.VAL",
      "+putorder": 1
    },
    "sa[0].enum": {
      "+channel": "george:enumExample.VAL",
      "+putorder": 0
    },
    "_save": {
      "+channel": "george:structExampleSave.VAL",
      "+type": "proc",
      "+putorder": 2,
      "+trigger": "*"
    }
  }
}
