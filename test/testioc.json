/*
 * This file defines two groups equivalent to the groups defined in testiocg.db
 * Dependencies: testioc.db
 */

/*
 * Group tst:tableExample2
 * Use +id mapping: to present as a epics NTTable
 * with two columns of doubles taken from $(user):vectorExampleD1 and $(user):vectorExampleD2
 * Get metadata (alarms, etc) from $(user):vectorExampleD1
 */
{
  "tst:tableExample2": {
    "+id": "epics:nt/NTTable:1.0",
    "": {
      "+type": "meta",
      "+channel": "tst:vectorExampleD1.VAL"
    },
    "labels": {
      "+channel": "tst:groupExampleAS.VAL",
      "+type": "plain"
    },
    "value.jA": {
      "+channel": "tst:vectorExampleD1.VAL",
      "+type": "plain",
      "+putorder": 1
    },
    "value.jB": {
      "+channel": "tst:vectorExampleD2.VAL",
      "+type": "plain",
      "+putorder": 1
    },
    "_save": {
      "+channel": "tst:groupExampleSave.VAL",
      "+type": "proc",
      "+putorder": 2,
      "+trigger": "*"
    }
  },
  /*
   * Group tst:structExample2
   * This is an example of a structure that does not correspond to an EPICS normative type
   */
  "tst:structExample2": {
    "jai": {
      "+channel": "tst:aiExample.VAL",
      "+putorder": 0
    },
    "jcalc": {
      "+channel": "tst:calcExample.VAL",
      "+type": "plain",
      "+putorder": 1
    },
    "jstring": {
      "+channel": "tst:stringExample.VAL",
      "+putorder": 0
    },
    "jarray": {
      "+channel": "tst:arrayExample.VAL",
      "+putorder": 0
    },
    "jsa[0].any": {
      "+channel": "tst:longExample.VAL",
      "+type": "any",
      "+putorder": 1
    },
    "jsa[1].enum": {
      "+channel": "tst:enumExample.VAL",
      "+putorder": 0
    },
    "_save": {
      "+channel": "tst:structExampleSave.VAL",
      "+type": "proc",
      "+putorder": 2,
      "+trigger": "*"
    }
  }
}
